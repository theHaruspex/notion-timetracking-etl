# Notion API
NOTION_TOKEN=secret_xxx

# Power BI Service Principal (required for PBI provision/sink commands)
PBI_TENANT_ID=your-tenant-id-here
PBI_CLIENT_ID=your-service-principal-id-here
PBI_CLIENT_SECRET=your-service-principal-secret-here

# Power BI provisioning targets (required for pbi:provision)
PBI_WORKSPACE_ID=your-workspace-id-here
PBI_DATASET_NAME=notion_timetracking_dev

# Optional: used by transplanted SDK diagnostics tooling
PBI_GROUP_ID=your-workspace-group-id-here
